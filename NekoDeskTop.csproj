<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Using\**" />
    <EmbeddedResource Remove="Using\**" />
    <None Remove="Using\**" />
    <Page Remove="Using\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Resources\back.png" />
    <None Remove="Resources\forward.png" />
    <None Remove="Resources\refresh.png" />
    <None Remove="Resources\search.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\back.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\forward.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\refresh.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\search.png" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="SettingPages\" />
  </ItemGroup>

</Project>
